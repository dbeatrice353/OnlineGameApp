

<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><b>Player</b></td>
      <td><b>Best Score</b></td>
      <td><b>Attempts</b></td>
    </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.user_name %></td>
        <td><%= user.max_score %></td>
        <td><%= user.attempt_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>